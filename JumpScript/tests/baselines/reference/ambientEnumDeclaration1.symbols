// In ambient enum declarations, all values specified in enum member declarations must be classified as constant enum expressions.

declare enum E {
>E : Symbol(E, Decl(ambientEnumDeclaration1.jump, 0, 0))

    a = 10,
>a : Symbol(E.a, Decl(ambientEnumDeclaration1.jump, 2, 16))

    b = 10 + 1,
>b : Symbol(E.b, Decl(ambientEnumDeclaration1.jump, 3, 11))

    c = b,
>c : Symbol(E.c, Decl(ambientEnumDeclaration1.jump, 4, 15))
>b : Symbol(E.b, Decl(ambientEnumDeclaration1.jump, 3, 11))

    d = (c) + 1,
>d : Symbol(E.d, Decl(ambientEnumDeclaration1.jump, 5, 10))
>c : Symbol(E.c, Decl(ambientEnumDeclaration1.jump, 4, 15))

    e = 10 << 2 * 8,
>e : Symbol(E.e, Decl(ambientEnumDeclaration1.jump, 6, 16))
}
