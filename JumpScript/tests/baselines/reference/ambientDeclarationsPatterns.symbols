// Augmentations still work
declare module "foo*baz" {
    export const baz: string;
>baz : Symbol(baz, Decl(declarations.d.jump, 5, 16))
}

// Longest prefix wins
declare module "foos*" {
    export const foos: string;
>foos : Symbol(foos, Decl(declarations.d.jump, 10, 16))
}

declare module "*!text" {
    const x: string;
>x : Symbol(x, Decl(declarations.d.jump, 14, 9))

    export default x;
>x : Symbol(x, Decl(declarations.d.jump, 14, 9))
}

