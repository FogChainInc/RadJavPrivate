// When symlinked files are not in node_modules, realpath is not used.
// A symlink file acts like the real thing. So, 2 symlinks act like 2 different files.
// See GH#10364. 
// @noImplicitReferences: true
// @traceResolution: true
// @fullEmitPaths: true

// @filename: /shared/abc.jump
// @symlink: /src/shared/abc.jump,/src/shared2/abc.jump
export const x = 0;

// @filename: /src/app.jump
import { x } from "./shared/abc";
import { x as x2 } from "./shared2/abc";
x + x2;

// @filename: /src/jmpconfig.json
{
    "compilerOptions": {
        "outDir": "bin"
    }
}

/*
# To reproduce in a real project:

mkdir shared
echo 'export const x = 0;' > shared/abc.jump

mkdir src; cd src
echo 'import { x } from "./shared/abc"; import { x as x2 } from "./shared2/abc"; x + x2;' > app.jump

ln -s ../shared ./shared; ln -s ../shared ./shared2 # Linux
# Windows: Open command prompt as administrator and run: `mklink /D shared ..\shared; mklink /D shared2 ..\shared`

echo '{ "compilerOptions": { "outDir": "bin" } }' > jmpconfig.json
jmpc # Should create `bin/app.js`, `bin/shared/abc.js`, and `bin/shared2/abc.js`
*/
