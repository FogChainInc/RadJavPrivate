+		//class ExternalOwningOneByteStringResource
+		//	: public v8::String::ExternalOneByteStringResource {
+		//public:
+		//	ExternalOwningOneByteStringResource() : length_(0) {}
+		//	ExternalOwningOneByteStringResource(std::unique_ptr<const char[]> data,
