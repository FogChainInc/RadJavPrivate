+			result.z = this.z + vector3.z;\n\
+		}\n\
+		else\n\
+		{\n\
+			result.x = this.x + vector3;\n\
+			result.y = this.y + vector3;\n\
+			result.z = this.z + vector3;\n\
+		}\n\
+\n\
+		return (result);\n\
