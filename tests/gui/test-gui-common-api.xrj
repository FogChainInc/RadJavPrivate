			let exceptionCaught = false;
			
			let promise = new Promise(function(resolve, reject) {
				var window = new RadJav.GUI.Window("window", controlName+" create after create API test");
				window.create().then(function(win) {
					window = win;
					
					var control = new RadJav.GUI[controlName](controlName, "test", window);
					control.create().then(function(ctrl) {
						control = ctrl;
						
						try
						{
							control.create().then(function(ctrl2) {
								control = ctrl2;
							});
						}
						catch(err)
						{
							exceptionCaught = true;
						}
						finally
						{
							check.assert(exceptionCaught, message);
							resolve();
						}
					});
				});
				
				setTimeout(function ()
				{
					check.assert(exceptionCaught, message);
					resolve();
				}, 5000);
			});
			
			return promise;
		}
		
