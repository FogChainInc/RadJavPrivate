		let setGetVisibilityApiTest = function(controlName) {
			let check = this;
			
			let steps = [];
			steps.push({executed: false, message: controlName+" is visible by default"});
			steps.push({executed: false, message: controlName+" not visible with setVisibility(false)"});
			steps.push({executed: false, message: controlName+" visible with setVisibility(true)"});
			
			let promise = new Promise(function(resolve, reject) {
				var window = new RadJav.GUI.Window("Window", controlName+" set/getVisibility API test");
				window.create().then(function(win) {
					window = win;
					window.setPosition(0, 22);
					
					var control = new RadJav.GUI[controlName](controlName, "test", window);
					control.create().then( function(ctl) {
						control = ctl;
						
						check.equal(control.getVisibility(), true, steps[0].message);
						steps[0].executed = true;
						
						setTimeout(function () {
							control.setVisibility(false);
							check.equal(control.getVisibility(), false, steps[1].message);
							steps[1].executed = true;
							
							setTimeout(function () {
								control.setVisibility(true);
								check.equal(control.getVisibility(), true, steps[2].message);
								steps[2].executed = true;
								
								setTimeout(function () {
									resolve();
								}, 1000);
							}, 1000);
						}, 1000);
					});
				});
				
				setTimeout(function ()
				{
					for(let i = 0; i < steps.length; i++)
					{
						if (!steps[i].executed)
						{
							check.assert(false, steps[i].message);
						}
					}
					resolve();
				}, 5000);
			});
			
			return promise;
		}
		
